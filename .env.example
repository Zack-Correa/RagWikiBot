# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
CLIENT_ID=your_bot_client_id_here
BOT_USER_ID=your_bot_user_id_here
GUILD_ID=your_guild_id_here

# Divine Pride API
DIVINE_PRIDE_API_KEY=your_divine_pride_api_key_here

# Logging
LOG_LEVEL=INFO

# Admin Panel Configuration
ADMIN_PASSWORD=your_secure_admin_password_here
ADMIN_PORT=3000
ADMIN_HOST=0.0.0.0
SESSION_SECRET=your_session_secret_here

# Player Count Monitoring (Optional)
# Method 1: Automatic launcher authentication (EXPERIMENTAL)
# The bot will attempt to simulate the game launcher to obtain game tokens
# This may not work if the launcher API endpoints are unknown
# Configure credentials and the bot will try to discover the token endpoint
RO_PROBE_USERNAME=your_email@example.com
RO_PROBE_PASSWORD=your_password
# Optional: TOTP secret for 2FA (base32 format, same as used in shared accounts)
# If your account has 2FA enabled, provide the TOTP secret here
RO_PROBE_TOTP_SECRET=

# Method 2: Manual token from Wireshark (RECOMMENDED - most reliable)
# Extract token from Wireshark capture of game launcher login packet (0x0825)
# Token is the Base64 string at offset 92-417 in the packet
# Run: node extract-token.js <hex_dump> to extract easily
RO_AUTH_TOKEN=

# Salesforce Agentforce (Optional - for AI assistant plugin)
SALESFORCE_CLIENT_ID=your_salesforce_client_id
SALESFORCE_CLIENT_SECRET=your_salesforce_client_secret
SALESFORCE_INSTANCE_URL=https://your-instance.my.salesforce.com
AGENTFORCE_AGENT_ID=your_agentforce_agent_id
